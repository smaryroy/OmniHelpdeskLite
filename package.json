  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^2.0.7"
  },
  "dependencies": {
    "@material-ui/core": "4.11.3",
    "@material-ui/icons": "4.11.2",
    "@material-ui/lab": "4.0.0-alpha.57",
    "axios": "^0.18.0",
    "bootstrap": "^4.6.0",
    "express": "^4.16.3",
    "if-env": "^1.0.4",
